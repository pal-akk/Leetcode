int findMaxConsecutiveOnes(vector<int>& nums) {
        int n=nums.size();
        int k=0;
        vector<int> count;
        for(int i=0;i<n;i++){
            if(nums[i]==1){
                k+=1;
            }
            else if(nums[i]!=1){
                count.push_back(k);
                k=0;
            }
        }
        count.push_back(k);
        return *max_element(count.begin(),count.end());
        
    }
