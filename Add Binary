int i=a.size()-1;
        int j=b.size()-1;
        int carry=0;
        int sum=0;
        while(i>=0 || j>=0 || carry==1){
            int sum=carry;
            if(i>=0){
                sum+=a[i]-'0';
            }
            if(j>=0){
                sum+=b[i]-'0';
            }
            i--;
            j--;
            result.push_back((sum%2)+'0');
            carry=sum/2;
        }
        reverse(result.begin(),result.end());
        return result;
