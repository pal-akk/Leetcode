int sum1=0;
        int sum2=0;
        for(int i=1;i<=n;i++){
            if(i%m==0){
                sum1+=i;
            }
            else{
                sum2+=i;
            }
        }
        sum1=(sum2-sum1);
        return sum1;

// another solution
int sum=(n*(n+1))/2;
        int k=n/m; //no. of multiples
        int s=(m*k*(k+1)/2);
        int s1=sum-s;
        sum=s1-s;
        return sum;
