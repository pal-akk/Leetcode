int subarraySum(vector<int>& nums, int k) {
        int n= nums.size();
        int tym=0;
        int sum=0;
        unordered_map<int, int> count;
        count[0]=1;
        for(int i=0; i<n ;i++){
            sum+=nums[i];
            if(count.find(sum-k) != count.end()){
                tym+=count[sum-k];
            }
            count[sum]++;
        }        
        return tym;
    }
