bool isValid(string s) {
        stack<char> st;
        for(int i=0;i<s.size();i++){
            char c =s[i];
            if(c=='('|| c=='{'|| c=='['){
                st.push(c);
            }
            else{
                if(st.size()==0){
                    return false;

                }
                if((st.top()=='(' && c==')') ||
                (st.top()=='{' && c=='}') ||
                (st.top()=='[' && c==']')){
                    st.pop();
                }
                else{
                    return false;
                }
            }
        }
        return st.empty();
